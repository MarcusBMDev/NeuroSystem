<configuration>
  <system.webServer>
    <rewrite>
      <rules>
        
        <rule name="NeuroAgenda" stopProcessing="true">
          <match url="^agenda/(.*)" />
          <conditions logicalGrouping="MatchAll" trackAllCaptures="false" />
          <action type="Rewrite" url="http://localhost:3002/{R:1}" />
        </rule>

      </rules>
    </rewrite>
  </system.webServer>
</configuration>